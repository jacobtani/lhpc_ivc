<div class="container">
  <h1>User Details</h1>
  <%= form_with(model: @user, url: admin_user_path, local: true) do |form| %>
    <%= form.label :email %>
    <%= form.text_field :email, class: "form-control", readonly:true %>
  <% end %>
  <%= link_to 'Back', admin_users_path,class:"btn btn-light min-width-btn" %>
</div>
